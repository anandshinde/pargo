{
	"name": "mohawkflooring",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve --port 4400",
		"build": "ng build",
		"test": "ng test",
		"lint": "ng lint",
		"lint:prettier": "prettier \"src/**/*.ts\"  --check",
		"format:prettier": "prettier \"src/**/*.ts\" --write",
		"e2e": "ng e2e",
		"dev:ssr": "ng run mohawkflooring:serve-ssr",
		"build:ssr": "ng build --prod && ng run mohawkflooring:server:production",
		"build-dev:ssr": "ng build --configuration=dev && ng run mohawkflooring:server:dev",
		"build-staging:ssr": "ng build --configuration=staging && ng run mohawkflooring:server:staging",
		"postinstall": "ngcc",
		"prepare": "husky install"
	},
	"private": true,
	"dependencies": {
		"@angular/animations": "~12.0.1",
		"@angular/cdk": "^12.0.3",
		"@angular/common": "^12.1.0",
		"@angular/compiler": "~12.0.1",
		"@angular/core": "^12.1.0",
		"@angular/forms": "^12.0.5",
		"@angular/google-maps": "^12.1.4",
		"@angular/material": "^12.0.3",
		"@angular/platform-browser": "~12.0.1",
		"@angular/platform-browser-dynamic": "~12.0.1",
		"@angular/platform-server": "~12.0.1",
		"@angular/router": "~12.0.1",
		"@angular/service-worker": "~12.0.1",
		"@bloomreach/ng-sdk": "14.4.0",
		"@nguniversal/builders": "^12.0.0",
		"@nguniversal/express-engine": "^12.0.0",
		"@videogular/ngx-videogular": "^3.0.1",
		"angular-calendar": "^0.28.22",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"date-fns": "^2.23.0",
		"idb-keyval": "^5.1.1",
		"masonry-layout": "^4.2.2",
		"ngx-chips": "2.2.2",
		"ngx-ellipsis": "^3.1.4",
		"ngx-pagination": "^5.1.1",
		"ngx-swiper-wrapper": "^10.0.0",
		"postcss": "^8.3.0",
		"react-is": "^17.0.2",
		"rxjs": "^6.6.7",
		"stylelint-config-airbnb": "0.0.0",
		"stylelint-order": "^4.1.0",
		"swiper": "^6.7.0",
		"tslib": "^2.3.0",
		"webpack-bundle-analyzer": "^4.4.2",
		"zone.js": "^0.11.4"
	},
	"devDependencies": {
		"@angular-devkit/build-angular": "^12.0.5",
		"@angular-eslint/builder": "^12.0.0",
		"@angular-eslint/eslint-plugin": "12.0.0",
		"@angular-eslint/eslint-plugin-template": "12.0.0",
		"@angular-eslint/schematics": "12.0.0",
		"@angular-eslint/template-parser": "12.0.0",
		"@angular/cli": "~12.0.1",
		"@angular/compiler-cli": "~12.0.1",
		"@types/jasmine": "~3.6.0",
		"@types/jasminewd2": "~2.0.3",
		"@types/node": "^12.11.1",
		"@typescript-eslint/eslint-plugin": "4.23.0",
		"@typescript-eslint/parser": "4.23.0",
		"eslint": "^7.26.0",
		"eslint-plugin-jasmine": "4.1.1",
		"husky": "^6.0.0",
		"jasmine-core": "^3.7.1",
		"jasmine-spec-reporter": "~5.0.0",
		"karma": "^6.3.4",
		"karma-chrome-launcher": "~3.1.0",
		"karma-coverage-istanbul-reporter": "~3.0.2",
		"karma-jasmine": "~4.0.0",
		"karma-jasmine-html-reporter": "^1.5.0",
		"modern-css-reset": "^1.4.0",
		"ng-mocks": "^12.0.2",
		"prettier": "^2.3.0",
		"protractor": "~7.0.0",
		"schematics-scss-migrate": "^1.3.13",
		"stylelint": "^13.13.1",
		"stylelint-config-prettier": "^8.0.2",
		"stylelint-config-sass-guidelines": "^7.1.0",
		"stylelint-scss": "^3.20.1",
		"ts-node": "~8.3.0",
		"tslint-config-prettier": "^1.18.0",
		"tslint-plugin-prettier": "^2.3.0",
		"typescript": "~4.2.4"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.+(ts)": [
			"eslint"
		],
		"*.+(ts|json|css|md)": [
			"prettier --write"
		]
	}
}
